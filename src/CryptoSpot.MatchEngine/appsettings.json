{
  "ApiService": {
    "BaseUrl": "http://localhost:5000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "CryptoSpot.MatchEngine": "Information"
    }
  },
  "Urls": "http://localhost:5001",
  "AllowedHosts": "*",
  "HealthChecks": {
    "EnableStartupCheck": true,
    "FailFast": true,
    "MaxRetries": 3,
    "RetryDelaySeconds": 5
  },
  "Redis": {
    "Password": "",
    "AllowAdmin": true,
    "Ssl": false,
    "ConnectTimeout": 6000,
    "ConnectRetry": 3,
    "Database": 2,
    "PoolSize": 80,
    "IdleTimeout": 30000,
    "WriteBuffer": 1024000,
    "PreHeat": 20,
    "Hosts": [
      {
        "Host": "127.0.0.1",
        "Port": "6379"
      }
    ]
  }
}
